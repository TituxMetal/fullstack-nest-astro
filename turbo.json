{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "docker:build": {
      "cache": false,
      "dependsOn": ["^docker:build"]
    },
    "docker:push": {
      "cache": false,
      "dependsOn": ["lint", "test", "docker:build"]
    },
    "lint": {},
    "start": {
      "dependsOn": ["build"]
    },
    "test": {}
  }
}
